
  <pre><code>
    //Get paste.
    const axios = require('axios');
    axios.get('https://emrahpaste.tk/api/get/:id')
    .then(function (response) {
      console.log(response.data);
    })
    .catch(function (error) {
      console.log(error);
    })

    //Save paste.
    axios.post('https://emrahpaste.tk/api/post/', {
      paste: 'You paste.'
    })
    .then(function (response) {
      console.log(response.data);
    })
    .catch(function (error) {
      console.log(error);
    });
  </code></pre>

